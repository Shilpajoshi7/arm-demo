    "ubuntuOSVersion": {
      "value": "18.04-LTS"
    },
    "location": {
      "value": "[resourceGroup().location]"
    },
    "vmSize": {
      "value": "Standard_B2s"
    },
    "virtualNetworkName": {
      "value": "vNet"
    },
    "subnetName": {
      "value": "Subnet"
    },
    "networkSecurityGroupName": {
      "value": "SecGroupNet"
    }
  }
}
