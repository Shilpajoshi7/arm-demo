{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vmName": {
      "value": "simpleLinuxVM"
    },
    "adminUsername": {
      "value": "shilpa"
    },
    "authenticationType": {
      "value": "password"
    },
    "adminPasswordOrKey": {
      "value": "Abc@12345678"
    },
    "dnsLabelPrefix": {
      "value": "[toLower(format('{0}-{1}', parameters('vmName'), uniqueString(resourceGroup().id)))]"
